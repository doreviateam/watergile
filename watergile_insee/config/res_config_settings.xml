<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.insee</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form" position="inside">
                <setting string="INSEE" help="Configuration de l'API INSEE">
                    <field name="insee_consumer_key" password="True"/>
                    <field name="insee_consumer_secret" password="True"/>
                    <field name="insee_update_interval"/>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>


